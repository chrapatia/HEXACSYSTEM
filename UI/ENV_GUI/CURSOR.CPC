// USER
CCP/ /SET.LEVEL = 3

// ---- Cursor ----

// [FR] Le curseur graphique
// [EN] Graphic cursor
SET/ CPC_GUI.CURSOR_IMG = OS/HEXAC/UI/GRAPHICS/GUI\CURSOR\CURSOR.PNG

// [FR] Le curseur graphique de chargement
// [EN] Graphic loading cursor
SET/ CPC_GUI.LOAD_IMG = OS/HEXAC/UI/GRAPHICS/GUI\CURSOR\LOAD.PNG


// [FR] Vitesse MAX / MIN
// [EN] MAX / MIN speed
SET/ CPC_GUI.CURSOR_MAX_SPEED = 5.0
SET/ CPC_GUI.CURSOR_MIN_SPEED = 10.0

// [FR] Vitesse inertielle
// [EN] Inertia speed
SET/ CPC_GUI.CURSOR_INERTIA_SPEED = 300.0
SET/ CPC_GUI.CURSOR_CONSTANT_SPEED = 5.0

// [FR] Espacement entre le curseur et sablier
// [EN] Space between cursor and load cursor
SET/ CPC_GUI.LOAD_PX = 14
SET/ CPC_GUI.LOAD_PY = 0







